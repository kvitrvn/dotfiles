Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

!*.foreground:    #A9B1D6
!*.background:    #1A1B26
!*.cursorColor:   #A9B1D6
!*.highlightColor:#3A3F4B

! Normal      Bright
